// 2차원 배열처리
public class Exam_03 {
	public static void main(String[] args) {
		int score[][]= new int[5][5];
		int cnt=0;
		for(int x=0; x<score.length; x++) {
			for(int y=0; y<score[x].length; y++) {
				cnt++;
				score[x][y]=cnt;
			}
		}
		//출력
		for(int x=0; x<score.length; x++) {
			for(int y=0; y<score[x].length; y++) {
				System.out.print(score[x][y]+"\t");
			}
			System.out.print("\n");
		}
		
	}

}
