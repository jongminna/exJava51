import java.util.Scanner;

public class Exam_08 {
	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		int cnt=0;
		System.out.print("자료입력:");
		int score;
		while(true) {
			score = scn.nextInt();
			if(score==-99) {
				break;
			}
		}
		System.out.println("score="+score);
	}

}
