import java.util.Arrays;

public class Exam_06 {
	public static void main(String[] args) {
		int var[] = {1,2,3,4,5};
		int temp[] = var;
		
		System.out.println(Arrays.toString(var));
		System.out.println(Arrays.toString(temp));
		
		var[1]=100;
		System.out.println(Arrays.toString(var));
		System.out.println(Arrays.toString(temp));
		
		
	}

}
